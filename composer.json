{
	"name": "dronemill/foundationapi",
	"description": "Foundational magic for running an api within Laravel 5",
	"keywords": ["laravel", "api", "dronemill"],
	"authors": [
		{
			"name": "Patrick McCarren",
			"email": "patrick@dronemill.com"
		}
	],
	"license": "MIT",
	"require": {
		"echo-it/laravel-jsonapi": "2.0.*",
		"dronemill/phphelpers": "dev-master"
	},
	"require-dev": {
		"mockery/mockery": "0.9.1",
		"league/factory-muffin": "3.0.*@dev",
		"league/factory-muffin-faker": "~1.0-dev"
	},
	"repositories": [
		{
			"type": "vcs",
			"url":  "git@github.com:dronemill/phphelpers.git"
		},
		{
			"type": "vcs",
			"url":  "git@github.com:dronemill/laravel-jsonapi.git"
		}
	],
	"minimum-stability": "dev",
	"autoload": {
		"psr-4": {
			"DroneMill\\FoundationApi\\": "src/"
		}
	}
}
